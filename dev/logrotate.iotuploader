/opt/iotuploader/log/dev_access.log
/opt/iotuploader/log/dev_error.log {
  su iotuploader iotuploader
  missingok
  ifempty
  daily
  rotate 90
  create 644 iotuploader iotuploader
  compress
  delaycompress
  sharedscripts
  postrotate
    kill -s USR1 $(cat /opt/iotuploader/run/dev_iotuploader.pid)
  endscript
}

