#! /usr/bin/bash -eu

DATA=$(cat << EOS
EP01_TEST_01,0x0,TEST,
EP01_TEST_01,0x1,TEST,
EP01_TEST_01,0x2,TEST,
EP01_TEST_01,0x3,TEST,
EP01_TEST_01,0x4,TEST,
EP01_TEST_01,0x5,TEST,
EP01_TEST_01,0x6,TEST,
EP01_TEST_01,0x7,TEST,
EP01_TEST_01,0x8,TEST,
EP01_TEST_01,0x186A0,TEST,
EOS
)

curl -X POST http://localhost:8100/upload/sensordata --data "${DATA}"

